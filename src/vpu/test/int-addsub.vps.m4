|* Integer addition and subtraction, which overlap code paths to a much
|* greater degree than natural number addition and subtraction.
	LDLc	RE, ' '
	LDLc	RF, '\n'

m4_define(`header',
`	LDLs	RA, "$1:\n"
	PRINTs	RA
')
m4_define(`testcase',
`	LDLz	R1, $1
	LDLz	R2, $2
	MOV	R0, R1
	ADDz	R0, R2		|* R1 + R2
	PRINTz	R0
	PRINTc	RE
	MOV	R0, R2
	ADDz	R0, R1		|* R2 + R1
	PRINTz	R0
	PRINTc	RE
	MOV	R0, R1
	SUBz	R0, R2		|* R1 - R2
	PRINTz	R0
	PRINTc	RE
	MOV	R0, R2
	SUBz	R0, R1		|* R2 - R1
	PRINTz	R0
	PRINTc	RF
')

header(Addition/subtraction special cases)
testcase(+0, +0)
testcase(+0, +1)
testcase(+0, -1)
testcase(+1, -1)

header(Addition/subtraction small integers)
testcase(+0, +34)
testcase(+0, -34)
testcase(+22, +34)
testcase(+22, -34)
testcase(-22, -34)
testcase(+65, +65)
testcase(+65, -65)
testcase(-65, -65)

header(Addition/subtraction near word boundary)
testcase(+4294967295, +1)	|* max uint32
testcase(+4294967295, -1)	|* max uint32
testcase(+4294967296, +1)	|* 2^32
testcase(+4294967296, -1)	|* 2^32

header(Regression test cases)
	|* This case isn't actually a regression test, since the
	|* failure had to do with GC clobbering a pointer and not
	|* the values of these numbers... but it's a good test to
	|* have here regardless.
	LDLz	R0, -
	LDLz	R1, 
	SUBz	R0, R1
	PRINTz	R0
	PRINTc	RF

	HALT
