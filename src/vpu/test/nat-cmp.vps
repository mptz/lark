|* nat comparisons.  We need to make sure we're comparing both values with
|* different numbers of limbs and large values with the same number of limbs,
|* as those are separate code paths.  Also note that to get into the comparison
|* logic, we need to get past the initial pointer comparison, as for example
|*	CMPn	R4, R4
|* will always immediately return 0 due to identical pointers.  To subvert
|* literal pooling we'll use two separate numbers and increment one to the
|* other--this will guarantee separate heap objects.
	LDLc	RE, ' '
	LDLc	RF, '\n'

	LDLs	RD, "Pointer-comparison path:\n"
	PRINTs	RD

	LDLn	R0, 0
	CMPn	R0, R0
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	LDLn	R0, 1
	CMPn	R0, R0
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	LDLn	R0, 123456
	CMPn	R0, R0
	LDRR	RA
	PRINTo	RA
	PRINTc	RF

	LDLs	RD, "Single-limb values:\n"
	PRINTs	RD

	LDLn	R0, 0
	LDLn	R1, 1
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	CMPn	R1, R0
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	DECn	R1
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RF

	LDLn	R0, 1
	LDLn	R1, 2
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	CMPn	R1, R0
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	DECn	R1
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RF

	LDLn	R0, 123456
	LDLn	R1, 123457
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	CMPn	R1, R0
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	DECn	R1
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RF

	LDLn	R0, 4000000000
	LDLn	R1, 4000000001
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	CMPn	R1, R0
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	DECn	R1
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RF

	LDLs	RD, "Different numbers of limbs in values:\n"
	PRINTs	RD

	LDLn	R0, 5555
	LDLn	R1, 55555555555555555555555555555555555555555555555555555
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	CMPn	R1, R0
	LDRR	RA
	PRINTo	RA
	PRINTc	RF

	LDLn	R0, 9999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999999
	LDLn	R1, 2222222222222222222222222222
	CMPn	R1, R0
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RF

	LDLs	RD, "Same numbers of limbs in values:\n"
	PRINTs	RD

	LDLn	R0, 
	LDLn	R1, 
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	CMPn	R1, R0
	LDRR	RA
	PRINTo	RA
	PRINTc	RF

	LDLn	R0, 
	LDLn	R1, 
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	CMPn	R1, R0
	LDRR	RA
	PRINTo	RA
	PRINTc	RF

	LDLn	R0, 
	LDLn	R1, 
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	DECn	R1
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RE
	DECn	R1
	CMPn	R0, R1
	LDRR	RA
	PRINTo	RA
	PRINTc	RF

	HALT
